---
import SearchBar from "./Searchbar";
import { queryDB } from "./Dbrouter"

const [digestData, tagData] = await Promise.all([
  queryDB("http://" + process.env.BACKEND_HOST + ":" + process.env.BACKEND_PORT + "/digest", "/digest"),
  queryDB("http://" + process.env.BACKEND_HOST + ":" + process.env.BACKEND_PORT + "/tags", "/tags")
 ]);
---

<SearchBar client:load blogArr={digestData} tags={tagData} />